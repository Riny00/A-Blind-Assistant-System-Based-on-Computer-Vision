
#menu_top,#menu_bottom
{
    background-color: #b1bffc;
}

#label
{
    border-image:url(:/res/pic/computer.png);
    margin-top:5px;
}
#label_2
{
    color:#3f6183;
    font: 16pt "宋体";
}
#label_3,#label_16
{
    color:#3f6183;
    font: 12pt "宋体";
}
